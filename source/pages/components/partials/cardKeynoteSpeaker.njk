{% set data = global %}
<div class="card speakers-card--keynote">
	<img src="{{global.headshot}}" class="speakers-card__headshot">
	<span class="card__header speakers-card__name">{{global.firstName}}<br>{{global.lastName}}</span>
	<span class="card__subhead speakers-card__position">{{global.position}}</span>
	<div class="speakers-card__backing-img">
		<img src="{{global.backingImg}}">
	</div>
</div>
